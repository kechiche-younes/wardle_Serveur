<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.StatGraphController">
    <Button text="Generate Graph" onAction="#generateGraph" />
    <LineChart fx:id="lineChart" title="Statistics Graph">
        <xAxis>
            <NumberAxis label="Index"/>
        </xAxis>
        <yAxis>
            <NumberAxis label="Values"/>
        </yAxis>
    </LineChart>
</VBox>
